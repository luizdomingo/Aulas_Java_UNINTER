package aulas_UNINTER;

import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args)
	{

		Scanner sc = new Scanner(System.in);
		Locale.setDefault(Locale.US);
				
		
		// Instanciando a class Curso
		Curso curso = new Curso();
		
		// Instanciando um Aluno
		Aluno aluno = new Aluno(curso);
		
		// Exibindo as informacoes
		aluno.info();
		
		//Quando usamos a class Scanner precisamos fecha apos o fim do algoritimo 
		sc.close();
	}

}
