	public static void main(String[] args)
	{

		Scanner sc = new Scanner(System.in);
		Locale.setDefault(Locale.US);
				
		
		
				
						
		//Quando usamos a class Scanner precisamos fecha apos o fim do algoritimo 
		sc.close();
	}

}
